@model AAS2237A5.Models.AccountDetails

@{
    ViewBag.Title = "Account Details";
}

<h2>Account Details</h2>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UserName)
        </dt>

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UserName)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ClaimsName)
        </dt>

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ClaimsName)
        </dd>

        <dt class="col-sm-2">
            Friendly name
        </dt>

        <dd class="col-sm-10">
            @string.Format("{0} {1}", Model.ClaimsGivenName, Model.ClaimsSurname)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ClaimsEmail)
        </dt>

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ClaimsEmail)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ClaimsRoles)
        </dt>

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ClaimsRoles)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Back to List", "Index")
</p>
